        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.ensemble import HistGradientBoostingClassifier\n",
        "from xgboost import XGBClassifier\n",
        "from lightgbm import LGBMClassifier\n",
        "from sklearn.model_selection import cross_validate\n",
        "import numpy as np\n",
        "\n",
        "X_sample_100k, _, y_sample_100k, _ = train_test_split(\n",
        "    X, y,\n",
        "    train_size=100000,\n",
        "    stratify=y,\n",
        "    random_state=42\n",
        ")\n",
        "\n",
        "print(f\"10Îßå Í∞ú Îç∞Ïù¥ÌÑ∞ Ï∂îÏ∂ú ÏôÑÎ£å: X({X_sample_100k.shape}), y({y_sample_100k.shape})\")\n",
        "\n",
        "top3_models = {\n",
        "    \"Hist Gradient Boosting\": HistGradientBoostingClassifier(random_state=42),\n",
        "    \"XGBoost\": XGBClassifier(random_state=42, n_jobs=-1),\n",
        "    \"LightGBM\": LGBMClassifier(random_state=42, n_jobs=-1)\n",
        "}\n",
        "\n",
        "print(\"10Îßå Í∞ú Îç∞Ïù¥ÌÑ∞Î°ú Top 3 Î™®Îç∏ Ï§ÄÍ≤∞ÏäπÏ†Ñ ÏãúÏûë!\\n\")\n",
        "\n",
        "results_100k = {}\n",
        "\n",
        "for name, model in top3_models.items():\n",
        "    print(f\"[{name}] 10Îßå Í∞ú Îç∞Ïù¥ÌÑ∞ ÌïôÏäµ Î∞è ÌèâÍ∞Ä Ï§ë...\")\n",
        "\n",
        "    cv_scores = cross_validate(model, X_sample_100k, y_sample_100k, cv=5, return_train_score=True, n_jobs=-1)\n",
        "\n",
        "    results_100k[name] = {\n",
        "        'Fit Time (Mean)': np.mean(cv_scores['fit_time']),\n",
        "        'Train Score': np.mean(cv_scores['train_score']),\n",
        "        'Validation Score': np.mean(cv_scores['test_score']),\n",
        "        'Validation Std': np.std(cv_scores['test_score'])\n",
        "    }\n",
        "\n",
        "    print(f\"  ÌèâÍ∑† ÌïôÏäµ ÏãúÍ∞Ñ: {results_100k[name]['Fit Time (Mean)']:.2f} Ï¥à\")\n",
        "    print(f\"  Train Ï†êÏàò: {results_100k[name]['Train Score']:.4f}\")\n",
        "    print(f\"  Validation Ï†êÏàò: {results_100k[name]['Validation Score']:.4f}\")\n",
        "    print(f\"  ÏÑ±Îä• Ìé∏Ï∞®(ÏïàÏ†ïÏÑ±): {results_100k[name]['Validation Std']:.4f}\\n\")\n",
        "\n",
        "print(\"Ï§ÄÍ≤∞ÏäπÏ†Ñ ÌèâÍ∞Ä ÏôÑÎ£å!\")"
