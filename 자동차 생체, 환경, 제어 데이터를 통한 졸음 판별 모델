    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "from xgboost import XGBClassifier\n",
        "from sklearn.model_selection import cross_validate, StratifiedKFold # Ï∂îÍ∞ÄÎê®\n",
        "\n",
        "# Îç∞Ïù¥ÌÑ∞Î•º ÏÑûÏñ¥Ï£ºÎäî 5-Fold Í∞ùÏ≤¥ ÏÉùÏÑ± (ÎπÑÏú® Ïú†ÏßÄ + ÏÖîÌîå) -> Ï≤òÏùåÏóî 58 ÎÇòÏôÄÏÑú ÍπúÏßù ÎÜÄÎûêÏùå. ShuffleÏùò Ï§ëÏöîÏÑ±ÏùÑ Îã§Ïãú ÌïúÎ≤à ÎäêÎÇå.\n",
        "skf = StratifiedKFold(n_splits=5, shuffle=True, random_state=42)\n",
        "\n",
        "final_model = XGBClassifier(random_state=42, n_jobs=-1)\n",
        "\n",
        "# 2. cv=5 ÎåÄÏã† Î∞©Í∏à ÎßåÎì† cv=skf ÎÑ£Ïùå.\n",
        "cv_results = cross_validate(final_model, X, y,\n",
        "                            cv=skf, # ÌïµÏã¨: ÏÖîÌîåÏù¥ Ï†ÅÏö©Îêú KFold ÏÇ¨Ïö©\n",
        "                            return_train_score=True,\n",
        "                            return_estimator=True,\n",
        "                            n_jobs=-1)\n",
        "\n",
        "val_scores = cv_results['test_score']\n",
        "print(f\"5-Fold ÌèâÍ∑† Í≤ÄÏ¶ù Ï†ïÌôïÎèÑ: {np.mean(val_scores):.4f} ({np.mean(val_scores)*100:.2f}%)\")\n",
        "print(f\"ÏÑ±Îä• Ìé∏Ï∞®(ÌëúÏ§ÄÌé∏Ï∞®): {np.std(val_scores):.4f}\")\n",
        "\n",
        "# ÌäπÏÑ± Ï§ëÏöîÎèÑ Ï∂îÏ∂ú Î∞è ÏãúÍ∞ÅÌôî\n",
        "estimators = cv_results['estimator']\n",
        "importances = np.mean([model.feature_importances_ for model in estimators], axis=0)\n",
        "\n",
        "fi_df = pd.DataFrame({\n",
        "    'Feature': X.columns,\n",
        "    'Importance': importances\n",
        "}).sort_values(by='Importance', ascending=False)\n",
        "\n",
        "plt.figure(figsize=(10, 6))\n",
        "sns.barplot(x='Importance', y='Feature', data=fi_df.head(10), palette='viridis')\n",
        "plt.title('Top 10 Feature Importances for Driver Alertness (XGBoost 5-Fold Avg)')\n",
        "plt.xlabel('Importance Score')\n",
        "plt.ylabel('Features')\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "print(\"[Top 10 Ï§ëÏöî ÌäπÏÑ±]\")\n",
        "print(fi_df.head(10).to_string(index=False))"
      ],
